{
  "name": "liv-portfolio",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "start:dev": "ng serve -c=development",
    "start:homolog-jit": "ng serve -c=homologation-jit",
    "start:homolog": "ng serve -c=homologation",
    "start:prod": "ng serve -c=production",
    "build": "ng build --configuration production",
    "config": "ts-node set-env.ts",
    "build:gcp": "npm run config && ng build --configuration homologation",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "prepare": "husky install",
    "commit": "git-cz"
  },
  "private": true,
  "engines": {
    "node": ">=16.15.x",
    "npm": ">=8.12.x"
  },
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/localize": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@ng-bootstrap/ng-bootstrap": "^12.1.2",
    "@ngneat/hot-toast": "^4.1.0",
    "@ngneat/overview": "^3.0.0",
    "@popperjs/core": "^2.10.2",
    "angular-svg-icon": "^13.0.0",
    "bootstrap": "^5.2.0",
    "colors": "^1.4.0",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.5",
    "dotenv": "^16.0.3",
    "marked": "^4.2.4",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.9",
    "@angular-eslint/builder": "13.5.0",
    "@angular-eslint/eslint-plugin": "13.5.0",
    "@angular-eslint/eslint-plugin-template": "13.5.0",
    "@angular-eslint/schematics": "13.5.0",
    "@angular-eslint/template-parser": "13.5.0",
    "@angular/cli": "~13.3.9",
    "@angular/compiler-cli": "~13.3.0",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@types/jasmine": "~3.10.0",
    "@types/marked": "^4.0.8",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "express": "^4.18.2",
    "husky": "^8.0.3",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
